{
  "name": "grzegorz-jamroz/plain-data-transformer",
  "description": "A PHP library for transforming various data to specific format.",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "PlainDataTransformer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PlainDataTransformerTests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.5",
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^10.4"
  }
}
